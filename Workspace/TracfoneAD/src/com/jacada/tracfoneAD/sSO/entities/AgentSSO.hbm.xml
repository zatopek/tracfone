<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jacada.lm.sso.model.AgentSSO" table="tf_agent_sso">
		<id name="ssoId" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>

		<property name="agentId" type="string">
			<column name="agent_id"/>
		</property>
		
		<property name="system">
			<column name="system_name" />
		</property>

		<set name="sSOCredentials" table="tf_agent_sso_credentials"
			 inverse="true" lazy="true" fetch="select">
			<key>
				<column name="id" not-null="true" />
			</key>
			<one-to-many class="com.jacada.tracfoneAD.sSO.entities.SSOCredential" />
		</set>
	
	</class>

</hibernate-mapping>