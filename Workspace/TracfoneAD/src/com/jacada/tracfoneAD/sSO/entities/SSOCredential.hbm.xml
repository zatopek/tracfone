<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jacada.tracfoneAD.sSO.entities.SSOCredential" table="tf_agent_sso_credentials">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>
        <many-to-one name="ssoId" class="com.jacada.tracfoneAD.sSO.entities.AgentSSO" fetch="select">
            <column name="ssoId" not-null="true" />
        </many-to-one>
		<property name="userId">
			<column name="sso_system_id"/>
		</property>
		<property name="password">
			<column name="sso_system_password" />
		</property>
	</class>
</hibernate-mapping>